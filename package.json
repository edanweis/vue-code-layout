{
  "name": "@edanweis/vue-code-layout",
  "version": "1.4.0-beta.1",
  "description": "A Vue editor layout component with built-in persistence",
  "main": "lib/vue-code-layout.umd.js",
  "module": "lib/vue-code-layout.es.js",
  "types": "lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/vue-code-layout.es.js",
      "require": "./lib/vue-code-layout.umd.js"
    },
    "./nuxt": {
      "types": "./lib/nuxt/index.d.ts",
      "import": "./lib/nuxt/module.mjs"
    },
    "./style.css": "./lib/vue-code-layout.css"
  },
  "nuxt": {
    "module": "lib/nuxt/module.mjs"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-lib": "vue-tsc --noEmit && vite build library",
    "build-demo": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.11"
  },
  "peerDependencies": {
    "nuxt": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nuxt/kit": "^3.9.0",
    "@types/node": "^20.10.4",
    "@vitejs/plugin-vue": "^4.5.2",
    "sass": "^1.69.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vue-tsc": "^1.8.25"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edanweis/vue-code-layout.git"
  },
  "keywords": [
    "vue",
    "layout",
    "editor",
    "vscode",
    "nuxt"
  ],
  "author": "edanweis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edanweis/vue-code-layout/issues"
  },
  "homepage": "https://github.com/edanweis/vue-code-layout#readme"
}
