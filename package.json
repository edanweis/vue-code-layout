{
  "name": "@edanweis/vue-code-layout",
  "version": "1.4.0-beta.7",
  "description": "A Vue editor layout component with built-in persistence",
  "main": "./lib/vue-code-layout.umd.js",
  "module": "./lib/vue-code-layout.es.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/vue-code-layout.es.js",
      "require": "./lib/vue-code-layout.umd.js"
    },
    "./nuxt": {
      "types": "./lib/nuxt/types/index.d.ts",
      "import": "./lib/nuxt/module.mjs"
    },
    "./composables": {
      "types": "./lib/composables/index.d.ts",
      "import": "./lib/composables/index.mjs"
    },
    "./style.css": "./lib/vue-code-layout.css",
    "./runtime/plugin": {
      "import": "./lib/nuxt/runtime/plugin.js"
    }
  },
  "files": [
    "lib",
    "nuxt",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build-lib && npm run build-nuxt",
    "build-lib": "vite build library",
    "build-demo": "vite build",
    "build-nuxt": "tsc -p tsconfig.nuxt.json --noEmit false --skipLibCheck true --noErrorTruncation false && node scripts/build-nuxt.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.11"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0",
    "nuxt": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    }
  },
  "devDependencies": {
    "@imengyu/vue3-context-menu": "^1.4.4",
    "@nuxt/kit": "^3.15.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/types": "^2.18.1",
    "@supabase/supabase-js": "^2.48.1",
    "@types/node": "^20.10.4",
    "@types/vue": "^1.0.31",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/runtime-core": "^3.5.13",
    "@vue/runtime-dom": "^3.5.13",
    "esbuild": "^0.24.2",
    "sass": "^1.69.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vite-plugin-dts": "^4.5.0",
    "vue-tsc": "^1.8.25"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edanweis/vue-code-layout.git"
  },
  "keywords": [
    "vue",
    "layout",
    "editor",
    "vscode",
    "nuxt"
  ],
  "author": "edanweis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edanweis/vue-code-layout/issues"
  },
  "homepage": "https://github.com/edanweis/vue-code-layout#readme"
}
